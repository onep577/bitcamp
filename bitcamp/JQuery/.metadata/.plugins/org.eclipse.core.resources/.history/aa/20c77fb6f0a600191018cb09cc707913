<script type="text/javascript">
/* file io */
var woman = [
	["", ""],
	["김영희", "01:06:11"],
	["이수빈", "01:16:45"],
	["박진희", "01:22:32"]
];

var man = [
	["", ""],
	["홍길동", "01:09:21"],
	["일지매", "01:12:34"],
	["정수동", "01:36:11"]
];

$(function () {
	
	//$("tr:even").css("background-color", "#00ff00");
	
//	$("tr:eq(1) td:eq(0)").html("데이터");
//	$("tr:eq(1) td:eq(1)").html("데이터1");

	$("#woman").click(function() {
		for(i = 0;i < 4; i++){
			for(j = 0;j < 2; j++){
				$("tr:eq(" + i + ") td:eq(" + j + ")").html(woman[i][j]);				
			}				
		}		
	});
	
	$("#man").click(function() {
		for(i = 0;i < 4; i++){
			for(j = 0;j < 2; j++){
				$("tr:eq(" + i + ") td:eq(" + j + ")").html(man[i][j]);				
			}				
		}		
	});
	
	$("td").mouseover(function() {
		$(this).css({"background-color":"#ffff00", "color":"#000"});
	});
	$("td").mouseout(function() {
	//	$(this).css("background-color", "#666666");
		$(this).css({"background-color":"#666666", "color":"#fff"});
	});
	
});

	$(".cls").css("", "");
	
	$(".myimg").addClass("cls");

</script>

</body>
