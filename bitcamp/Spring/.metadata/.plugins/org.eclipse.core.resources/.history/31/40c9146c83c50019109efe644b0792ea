	VALUES( #{id}, #{pwd}, #{name}, #{email}, 3)
</insert>

<select id="loginCheck" parameterType="bit.com.a.model.MemberDto" resultType="java.lang.String">
	SELECT ID
	FROM MEMBER
	WHERE ID=#{id} AND PWD=#{pwd}
</select>

